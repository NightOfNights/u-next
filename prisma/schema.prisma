generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:qwe@localhost:5432/u-store?schema=public"
}

model products {
  id          Int    @id @default(autoincrement())
  name        String
  price       Float
  rating      Float
  description String
  imageSrc    String
}
